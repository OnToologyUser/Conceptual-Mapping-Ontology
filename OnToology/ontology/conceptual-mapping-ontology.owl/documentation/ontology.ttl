@prefix : <http://vocab.linkeddata.es/def/conceptual-mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://vocab.linkeddata.es/def/conceptual-mapping#> .

<http://vocab.linkeddata.es/def/conceptual-mapping#> rdf:type owl:Ontology ;
                                                      <http://purl.org/dc/elements/1.1/publisher> <http://www.oeg-upm.net/> ;
                                                      <http://purl.org/dc/elements/1.1/title> "Conceptual Mapping"@en ;
                                                      <http://purl.org/dc/terms/contributor> "Andrea Cimmino (Ontology Engineering Group)"@es ,
                                                                                             "Edna Ruckhaus (Ontology Engineering Group)"@es ,
                                                                                             "Oscar Corcho (Ontology Engineering Group)"@es ,
                                                                                             "Raúl García Castro (Ontology Engineering Group)"@es ;
                                                      <http://purl.org/dc/terms/created> "2020-10-14"@en ;
                                                      <http://purl.org/dc/terms/creator> "Ana Iglesias Molina (Ontology Engineering Group)"@es ;
                                                      <http://purl.org/dc/terms/description> "TBD"@en ;
                                                      <http://purl.org/dc/terms/license> <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                                      <http://purl.org/dc/terms/modified> "2020-11-10" ;
                                                      <http://purl.org/vocab/vann/preferredNamespacePrefix> "cm"@en ;
                                                      <http://purl.org/vocab/vann/preferredNamespaceUri> <http://vocab.linkeddata.es/def/conceptual-mapping> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/publisher
<http://purl.org/dc/elements/1.1/publisher> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
<http://purl.org/dc/terms/modified> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/vocab/vann/hasSecurityScheme
<http://purl.org/vocab/vann/hasSecurityScheme> rdf:type owl:ObjectProperty ;
                                               rdfs:domain :DataAccess ;
                                               rdfs:range <https://www.w3.org/2019/wot/security#SecurityScheme> ;
                                               rdfs:comment "A relation between a data access and the security scheme it has"@en ;
                                               rdfs:label "Has security scheme"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#expects
:expects rdf:type owl:ObjectProperty ;
         rdfs:domain :EvaluableExpression ;
         rdfs:range :EvaluableExpression ;
         rdfs:comment "A relation between an Evaluable Expression and the Evaluable Expressions it takes as parameters"@en ;
         rdfs:label "Expects"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasArgument
:hasArgument rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :DataAccess
                                         :DataSource
                                       )
                         ] ;
             rdfs:range :Argument ;
             rdfs:comment "A relation between the data access or data source and an argument that describes it"@en ;
             rdfs:label "Has argument"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasDataAccess
:hasDataAccess rdf:type owl:ObjectProperty ;
               rdfs:domain :DataSource ;
               rdfs:range :DataAccess ;
               rdfs:comment "A relation between a data source and its data access"@en ;
               rdfs:label "Has data access"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasDataSource
:hasDataSource rdf:type owl:ObjectProperty ;
               rdfs:domain :RuleSet ;
               rdfs:range :DataSource ;
               rdfs:comment "A relation between a rule set and its data source"@en ;
               rdfs:label "Has data source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasDataType
:hasDataType rdf:type owl:ObjectProperty ;
             rdfs:domain :DataPropertyRule ;
             rdfs:range :EvaluableExpression ;
             rdfs:comment "A relation between a Data Property Object and its datatype"@en ;
             rdfs:label "Has data type"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasLanguage
:hasLanguage rdf:type owl:ObjectProperty ;
             rdfs:domain :StringPropertyRule ;
             rdfs:range :EvaluableExpression ;
             rdfs:comment "A relation between a string property rule and an evaluable expression that defines the language of the rule"@en ;
             rdfs:label "Has language"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasLinkingExpression
:hasLinkingExpression rdf:type owl:ObjectProperty ;
                      rdfs:domain :LinkingRule ;
                      rdfs:range :LinkingExpression ;
                      rdfs:comment "A relation between a linking rule and its linking expression"@en ;
                      rdfs:label "Has linking expression"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasObject
:hasObject rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :DataPropertyRule
                                       :ObjectPropertyRule
                                     )
                       ] ;
           rdfs:range :EvaluableExpression ;
           rdfs:comment "A relation between a data or object property rule and the object it defines with an evaluable expression"@en ;
           rdfs:label "Has object"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasPredicate
:hasPredicate rdf:type owl:ObjectProperty ;
              rdfs:domain :Rule ;
              rdfs:range :EvaluableExpression ;
              rdfs:comment "A relation between a data or object property rule and the predicate it defines with an evaluable expression"@en ;
              rdfs:label "Has predicate"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasPrefix
:hasPrefix rdf:type owl:ObjectProperty ;
           rdfs:domain :ConceptualMapping ;
           rdfs:range :Prefix ;
           rdfs:comment "A relation between the mapping and the prefixes it uses"@en ;
           rdfs:label "Has prefix"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasRule
:hasRule rdf:type owl:ObjectProperty ;
         rdfs:domain :RuleSet ;
         rdfs:range :Rule ;
         rdfs:comment "A relation between a rule set and the rules it contains"@en ;
         rdfs:label "Has rule"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasRuleSet
:hasRuleSet rdf:type owl:ObjectProperty ;
            rdfs:domain :ConceptualMapping ;
            rdfs:range :RuleSet ;
            rdfs:comment "A relation between a mapping and the rule sets it contains"@en ;
            rdfs:label "Has rule set"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasSubject
:hasSubject rdf:type owl:ObjectProperty ;
            rdfs:domain :RuleSet ;
            rdfs:range :EvaluableExpression ;
            rdfs:comment "A relation between a data or object property rule and the subject it defines with an evaluable expression"@en ;
            rdfs:label "Has subject"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#provenance
:provenance rdf:type owl:ObjectProperty ;
            rdfs:domain :ConceptualMapping ;
            rdfs:range :Ontology ;
            rdfs:comment "A relation between a mapping and the ontology of provenance"@en ;
            rdfs:label "Provenance"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#source
:source rdf:type owl:ObjectProperty ;
        rdfs:domain :LinkingRule ;
        rdfs:range :RuleSet ;
        rdfs:comment "A relation between a linking rule and its source rule set"@en ;
        rdfs:label "Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#target
:target rdf:type owl:ObjectProperty ;
        rdfs:domain :LinkingRule ;
        rdfs:range :RuleSet ;
        rdfs:comment "A relation between a linking rule and its target rule set"@en ;
        rdfs:label "Target"@en .


#################################################################
#    Data properties
#################################################################

###  http://vocab.linkeddata.es/def/conceptual-mapping#URI
:URI rdf:type owl:DatatypeProperty ;
     rdfs:domain [ rdf:type owl:Class ;
                   owl:unionOf ( :DataAccess
                                 :Ontology
                                 :Prefix
                               )
                 ] ;
     rdfs:range xsd:anyURI ;
     rdfs:comment "Uniform Resource Identifier used in a data access, ontology or prefix"@en ;
     rdfs:label "URI"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#delimiter
:delimiter rdf:type owl:DatatypeProperty ;
           rdfs:domain :TabularSource ;
           rdfs:range xsd:string ;
           rdfs:comment "A character that separates the different fields of a tabular source"@en ;
           rdfs:label "Delimiter"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#expression
:expression rdf:type owl:DatatypeProperty ;
            rdfs:domain :TextSource ;
            rdfs:range xsd:string ;
            rdfs:comment "What specifies which data is retrieven from a text source"@en ;
            rdfs:label "Expression"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#iterator
:iterator rdf:type owl:DatatypeProperty ;
          rdfs:domain :TreeSource ;
          rdfs:range xsd:string ;
          rdfs:comment "What is to be iterated in a tree source"@en ;
          rdfs:label "Iterator"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Argument
                                  :Ontology
                                )
                  ] ;
      rdfs:range xsd:string ;
      rdfs:comment "Name of the argument or property to be defined"@en ;
      rdfs:label "Name"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#prefix
:prefix rdf:type owl:DatatypeProperty ;
        rdfs:domain :Ontology ;
        rdfs:range xsd:string ;
        rdfs:comment "Abreviation of an URI that is used in the ontology"@en ;
        rdfs:label "Label"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#protocol
:protocol rdf:type owl:DatatypeProperty ;
          rdfs:domain :DataAccess ;
          rdfs:range xsd:string ;
          rdfs:comment "Protocol used to acces the data"@en ;
          rdfs:label "Protocol"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#query
:query rdf:type owl:DatatypeProperty ;
       rdfs:domain :DataSource ;
       rdfs:range xsd:string ;
       rdfs:comment "Query used to retrieve data in a data source"@en ;
       rdfs:label "Query"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#refreshTime
:refreshTime rdf:type owl:DatatypeProperty ;
             rdfs:domain :AsynchronousSource ;
             rdfs:range xsd:integer ;
             rdfs:comment "Time between every data request"@en ;
             rdfs:label "Refresh time"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#returnsValue
:returnsValue rdf:type owl:DatatypeProperty ;
              rdfs:domain :EvaluableExpression ;
              rdfs:range rdfs:Literal ;
              rdfs:comment "Value returned from an evaluable expression"@en ;
              rdfs:label "Returns value"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#separator
:separator rdf:type owl:DatatypeProperty ;
           rdfs:domain :TabularSource ;
           rdfs:range xsd:string ;
           rdfs:comment "Character that separates two values from one field in a tabular source"@en ;
           rdfs:label "Separator"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#value
:value rdf:type owl:DatatypeProperty ;
       rdfs:domain :Argument ;
       rdfs:range rdfs:Literal ;
       rdfs:comment "Value that takes an argument"@en ;
       rdfs:label "Value"@en .


#################################################################
#    Classes
#################################################################

###  http://vocab.linkeddata.es/def/conceptual-mapping#Argument
:Argument rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :name ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :value ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          rdfs:comment "Element that allows the definition of additional options for describing Data Access and Data Source"@en ;
          rdfs:label "Argument"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#AsynchronousSource
:AsynchronousSource rdf:type owl:Class ;
                    rdfs:subClassOf :DataSource ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :refreshTime ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:integer
                                    ] ;
                    owl:disjointWith :EventSource ,
                                     :SynchronousSource ;
                    rdfs:comment "Data source that is not synchronized when it is sent or received"@en ;
                    rdfs:label "Asynchronous Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Concatenate
:Concatenate rdf:type owl:Class ;
             rdfs:subClassOf :EvaluableExpression ;
             rdfs:comment "Expression that joins the values of two Evaluable Expressions"@en ;
             rdfs:label "Concatenate"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#ConceptualMapping
:ConceptualMapping rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPrefix ;
                                     owl:someValuesFrom :Prefix
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasRuleSet ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :RuleSet
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :provenance ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Ontology
                                   ] ;
                   rdfs:comment "Element that contains the necessary information to transform source data to RDF according to an ontology's schema"@en ;
                   rdfs:label "Conceptual Mapping"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Constant
:Constant rdf:type owl:Class ;
          rdfs:subClassOf :EvaluableExpression ;
          rdfs:comment "Expression that contains a constant value"@en ;
          rdfs:label "Constant"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#DataAccess
:DataAccess rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty <http://purl.org/vocab/vann/hasSecurityScheme> ;
                              owl:someValuesFrom <https://www.w3.org/2019/wot/security#SecurityScheme>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasArgument ;
                              owl:someValuesFrom :Argument
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :URI ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:anyURI
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :protocol ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            rdfs:comment "Description of the way of retrieving the data from its source"@en ;
            rdfs:label "Data Access"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#DataPropertyRule
:DataPropertyRule rdf:type owl:Class ;
                  rdfs:subClassOf :Rule ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasObject ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :EvaluableExpression
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasDataType ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :EvaluableExpression
                                  ] ;
                  rdfs:comment "Rule that contains the information to generate a data property"@en ;
                  rdfs:label "Data Property Rule"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#DataSource
:DataSource rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasArgument ;
                              owl:someValuesFrom :Argument
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasDataAccess ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :DataAccess
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :query ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            rdfs:comment "Description of the retrieved data"@en ;
            rdfs:label "Data Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#DataSourceAccess
:DataSourceAccess rdf:type owl:Class ;
                  rdfs:subClassOf :EvaluableExpression ;
                  rdfs:comment "Expression with a value retrieved from the source data"@en ;
                  rdfs:label "Data Source Access"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#EvaluableExpression
:EvaluableExpression rdf:type owl:Class ;
                     rdfs:subClassOf rdf:List ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :expects ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :EvaluableExpression
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :returnsValue ;
                                       owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                     ] ;
                     rdfs:comment "Element that contains the information to form objects, predicates and subjects"@en ;
                     rdfs:label "Evaluable Expression"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#EventSource
:EventSource rdf:type owl:Class ;
             rdfs:subClassOf :DataSource ;
             owl:disjointWith :SynchronousSource ;
             rdfs:label "Event Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#GraphSource
:GraphSource rdf:type owl:Class ;
             rdfs:subClassOf :DataSource ;
             rdfs:comment "Data source that is stored as a graph"@en ;
             rdfs:label "Graph Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#HTMLSource
:HTMLSource rdf:type owl:Class ;
            rdfs:subClassOf :TreeSource ;
            rdfs:comment "Data stored in HTML format"@en ;
            rdfs:label "HTML Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#JSONSource
:JSONSource rdf:type owl:Class ;
            rdfs:subClassOf :TreeSource ;
            rdfs:comment "Data stored in JSON format"@en ;
            rdfs:label "JSON Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#LinkingExpression
:LinkingExpression rdf:type owl:Class ;
                   rdfs:subClassOf :EvaluableExpression ;
                   rdfs:comment "Expression that defines how two Rule Sets are linked"@en ;
                   rdfs:label "Linking Expression"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#LinkingRule
:LinkingRule rdf:type owl:Class ;
             rdfs:subClassOf :Rule ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasLinkingExpression ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :LinkingExpression
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :source ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :RuleSet
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :target ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :RuleSet
                             ] ;
             rdfs:comment "Rule that contains the information to generate a link between two Rule Sets"@en ;
             rdfs:label "Linking Rule"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#ObjectPropertyRule
:ObjectPropertyRule rdf:type owl:Class ;
                    rdfs:subClassOf :Rule ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasObject ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :EvaluableExpression
                                    ] ;
                    rdfs:comment "Rule that contains the information to generate an object property"@en ;
                    rdfs:label "Object Property Rule"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Ontology
:Ontology rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :URI ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:anyURI
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :prefix ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdfs:comment "Ontology used guide the data translation"@en ;
          rdfs:label "Ontology"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Prefix
:Prefix rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :URI ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:anyURI
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :prefix ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        rdfs:comment "Prefix used in the mapping"@en ;
        rdfs:label "Prefix"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Rule
:Rule rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :hasPredicate ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :EvaluableExpression
                      ] ;
      rdfs:comment "Element that defines the properties and objects to be created"@en ;
      rdfs:label "Rule"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#RuleSet
:RuleSet rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasDataSource ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :DataSource
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasRule ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Rule
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasSubject ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :EvaluableExpression
                         ] ;
         rdfs:comment "Group of Rules that are related to a subject"@en ;
         rdfs:label "Rule Set"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#StringPropertyRule
:StringPropertyRule rdf:type owl:Class ;
                    rdfs:subClassOf :DataPropertyRule ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasLanguage ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :EvaluableExpression
                                    ] ;
                    rdfs:comment "Rule that contains the information to generate a data property with a string datatype"@en ;
                    rdfs:label "String Property Rule"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#SynchronousSource
:SynchronousSource rdf:type owl:Class ;
                   rdfs:subClassOf :DataSource ;
                   rdfs:comment "Data source that is synchronized when it is sent or received"@en ;
                   rdfs:label "Synchronous Source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#TabularSource
:TabularSource rdf:type owl:Class ;
               rdfs:subClassOf :DataSource ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :delimiter ;
